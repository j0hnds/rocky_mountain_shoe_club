<% form_for @show do | f | %>
<ol>
  <li><%= f.label :description %><%= f.text_field :description %></li>
  <li><%= f.label :coordinator_id %><%= f.text_field :coordinator_id %></li>
  <li><%= f.label :venue_id %><%= f.text_field :venue_id %></li>
  <li><%= f.label :start_date %><%= f.text_field :start_date %></li>
  <li><%= f.label :end_date %><%= f.text_field :end_date %></li>
  <li><%= f.label :next_start_date %><%= f.text_field :next_start_date %></li>
  <li><%= f.label :next_end_date %><%= f.text_field :next_end_date %></li>
  <li><%= f.submit %></li>
</ol>
<% end %>
