<% form_for @exhibitor, :html => { :method => :delete, :class => 'auxilliary ajaxed' } do |f| %>
<p><strong>Delete exhibitor: <%= format_name(@exhibitor) -%>?</strong></p>
  <% field_set_tag nil, :class => "button_wrapper" do %>
    <%= f.submit 'Delete', :class => "button" -%>
  <% end %>
<% end %>